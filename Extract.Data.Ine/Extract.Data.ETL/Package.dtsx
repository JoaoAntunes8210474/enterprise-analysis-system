    public void Main()
    {
        bool fileFound = false;
        string packageFileName = "Package.dtsx";

        try
        {
            // Check both localized and English paths
            string[] directoriesToCheck = { @"C:\Utilizadores", @"C:\Users" };

            foreach (string rootDirectory in directoriesToCheck)
            {
                if (Directory.Exists(rootDirectory))
                {
                    SearchDirectory(rootDirectory, packageFileName, ref fileFound);
                    if (fileFound) break;
                }
            }

            if (!fileFound)

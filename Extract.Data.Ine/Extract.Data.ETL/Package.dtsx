            {
                return;
            }

            // Log current directory
            Dts.Events.FireInformation(0, "FindPackage", $"Searching in directory: {directory}", string.Empty, 0, ref fileFound);

            // Check for the file in the current directory
            foreach (string file in Directory.GetFiles(directory, fileName, SearchOption.TopDirectoryOnly))
            {
                Dts.Variables["User::PackagePath"].Value = file;
                Dts.Events.FireInformation(0, "FindPackage", $"File found: {file}", string.Empty, 0, ref fileFound);
                fileFound = true;
                return;
            }

            // Recursively search subdirectories
            foreach (string subdirectory in Directory.GetDirectories(directory))
            {
                if (fileFound) return; // Stop searching if the file is found
                SearchDirectory(subdirectory, fileName, ref fileFound);
            }
        }
        catch (UnauthorizedAccessException ex)
        {
            // Log access denied warnings but continue execution
            Dts.Events.FireWarning(0, "FindPackage", $"Access denied to directory {directory}: {ex.Message}", string.Empty, 0);
        }
        catch (Exception ex)
        {
            // Catch any other exceptions
            Dts.Events.FireError(0, "FindPackage", $"Error searching directory {directory}: {ex.Message}", string.Empty, 0);
        }
    }

    public enum ScriptResults
    {
        Success = DTSExecResult.Success,
        Failure = DTSExecResult.Failure
    }
}]]></ProjectItem>

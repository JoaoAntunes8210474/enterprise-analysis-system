        }
        catch (Exception ex)
        {
            // Log unexpected errors
            Dts.Events.FireError(0, "FindPackage", ex.Message, string.Empty, 0);
            Dts.TaskResult = (int)ScriptResults.Failure;
        }
    }

    private void SearchDirectory(string directory, string fileName, ref bool fileFound)
    {
        try
        {
            // Skip system or hidden directories
            DirectoryInfo dirInfo = new DirectoryInfo(directory);
            if ((dirInfo.Attributes & FileAttributes.System) == FileAttributes.System ||
